#%RAML 1.0
title: session-2
types:
  SFDCPanther: !include schemas/newsdataType.raml
/search:
  get:
    queryParameters:
      keyword:
        type: string
        minLength: 3
        maxLength: 10

    responses:
      200:
        body:
          application/json:
            example: !include examples/searchExample.raml
              
  

/news:
  post:
    queryParameters:
      country:
        required: false
    body:
      application/json:
        type: SFDCPanther
    responses:
      201:
        body:
          application/json:
            example: !include examples/searchExample.raml
      
/sportsNews:
  put:
    body:
      application/json:
        type: SFDCPanther
    


